<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
       <bean id="question" class="com.spring.a2.Question">  
       <property name="questionId" value="1"></property>  
       <property name="questions" value="What is Java?"></property>  
       <property name="answers"> 
       <map>  
       <entry key="1"  value="Java is a programming language"></entry>  
       <entry key="2" value="Java is a Platform"></entry>  
       </map> 
     <!--   <set>  
       <value>Java is a programming language</value>  
       <value>Java is a platform</value>  
       <value>Java is an Island</value>  
       </set> -->  
       </property>  
       </bean>  
       
        </beans>